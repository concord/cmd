#!/bin/bash --login


case ${1} in
    config)
        shift
        python -m concord_cli/config "$@"
        ;;
    graph)
        shift
        python -m concord_cli/print_graph "$@"
        ;;
    deploy)
        shift
        python -m concord_cli/deploy "$@"
        ;;
    kill)
        shift
        python -m concord_cli/kill_task "$@"
        ;;
    *)
        echo "Usage: "
        echo "concord [program] [flags] "
        echo "concord (config|graph|kill|deploy) -h"
        ;;
esac
